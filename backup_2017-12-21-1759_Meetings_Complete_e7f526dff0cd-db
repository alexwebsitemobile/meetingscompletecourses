  `eMin` int(10) unsigned NOT NULL,
  `IP` binary(16) NOT NULL,
  `hits` int(10) unsigned NOT NULL,
  PRIMARY KEY (`eMin`,`IP`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1 ;

