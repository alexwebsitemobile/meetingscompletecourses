  `prerequisite_quiz_id` int(11) NOT NULL,
  `quiz_id` int(11) NOT NULL,
  PRIMARY KEY (`prerequisite_quiz_id`,`quiz_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 ;

# Data contents of table `wp_wp_pro_quiz_prerequisite`


